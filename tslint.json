{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "max-classes-per-file": [
      false
    ],
    "no-string-literal": false,
    "jsx-no-bind": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-console": [
      false
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}